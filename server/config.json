{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "url" : "http://localhost:3000/",
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  },
  "httpMode": false,
  "certConfig": {
    "path": "/etc/letsencrypt/archive/liquidpineapple.com",
    "key": "privkey1.pem",
    "cert": "cert1.pem",
    "ca": [],
    "requestCert": false,
    "rejectUnauthorized": false
  }
}
